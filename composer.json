{
    "name": "zbrettonye/hcaptcha",
    "description": "hCaptcha For Laravel5~7",
    "keywords": ["hcaptcha", "captcha", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "zbrettonye",
            "email": "monkeyblacktech97@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "^5.5 || ^6 || ^7",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "ZBrettonYe\\HCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZBrettonYe\\HCaptcha\\HCaptchaServiceProvider"
            ],
            "aliases": {
                "HCaptcha": "ZBrettonYe\\HCaptcha\\Facades\\HCaptcha"
            }
        }
    }
}
