{
  "name": "zbrettonye/hcaptcha",
  "description": "hCaptcha For Laravel6~8",
  "keywords": [
    "hcaptcha",
    "captcha",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zbrettonye",
      "email": "monkeyblacktech97@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.2|^7.0",
    "illuminate/support": "^6|^7|^8|^9"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8|^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "ZBrettonYe\\HCaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZBrettonYe\\HCaptcha\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZBrettonYe\\HCaptcha\\HCaptchaServiceProvider"
      ],
      "aliases": {
        "HCaptcha": "ZBrettonYe\\HCaptcha\\Facades\\HCaptcha"
      }
    }
  }
}
